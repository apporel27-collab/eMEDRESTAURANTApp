{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "Microsoft.EntityFrameworkCore.Database.Command": "None",
      "Microsoft.AspNetCore.Hosting": "Error",
      "Microsoft.AspNetCore.Mvc": "Error",
      "Microsoft.AspNetCore.Routing": "Error",
      "Microsoft.AspNetCore.StaticFiles": "Error",
      "Microsoft.AspNetCore.Authentication": "Error"
    }
  },
  "DetailedErrors": false,
  "Kestrel": {
    "Limits": {
      "MaxConcurrentConnections": 50,
      "MaxConcurrentUpgradedConnections": 50,
      "MaxRequestBodySize": 30000000,
      "KeepAliveTimeout": "00:00:30",
      "RequestHeadersTimeout": "00:00:30"
    }
  },
  "AllowedHosts": "*"
}
